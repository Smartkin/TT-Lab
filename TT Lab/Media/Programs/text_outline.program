fragment_program TextOutlineFragShader glsl glsles hlsl
{
    source text_outline.frag
    target ps_3_0
    default_params
    {
        param_named outlineColor float4 0.0 0.0 0.0 1.0
        param_named texelSize float2 0.03125 0.03125
        param_named outlineWidth float1 0.2
    }
}

vertex_program TextOutlineVertShader glsl glsles hlsl
{
    source text_outline.vert
    default_params
    {
        param_named_auto worldViewProj worldviewproj_matrix
    }
} 